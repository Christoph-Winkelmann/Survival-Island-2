<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="survival_island_2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:survival_island_2.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="Survival Island 2">

    <ShellContent
        Title="Main Menu"
        ContentTemplate="{DataTemplate view:MainMenuView}"
        Route="MainMenu"/>
    <ShellContent
        Title="Credits"
        ContentTemplate="{DataTemplate view:CreditsView}"
        Route="Credits"/>

</Shell>
